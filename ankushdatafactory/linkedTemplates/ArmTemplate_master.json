{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ankushdatafactory"
		},
		"Azure_Datalake_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Azure_Datalake_ls'"
		},
		"Azure_SQL_Tables_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azure_SQL_Tables'"
		},
		"ls_SQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_SQLServer'"
		},
		"Azure_Datalake_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azurebiglake01.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ankushdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azure_Datalake_ls_accountKey": {
						"value": "[parameters('Azure_Datalake_ls_accountKey')]"
					},
					"Azure_SQL_Tables_connectionString": {
						"value": "[parameters('Azure_SQL_Tables_connectionString')]"
					},
					"ls_SQLServer_connectionString": {
						"value": "[parameters('ls_SQLServer_connectionString')]"
					},
					"Azure_Datalake_ls_properties_typeProperties_url": {
						"value": "[parameters('Azure_Datalake_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}